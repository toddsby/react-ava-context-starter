{
  "name": "react-parcel",
  "version": "0.0.3",
  "description": "rapid react prototyping with parceljs",
  "browser": "main.js",
  "scripts": {
    "start": "parcel index.html --port 1678",
    "build": "rimraf dist && parcel build index.html --no-minify",
    "test": "ava \"./test/**/*-tests.js\" --verbose"
  },
  "author": "Drew Toddsby",
  "license": "UNLICENSED",
  "ava": {
    "require": [
      "./ava-config.js",
      "./jsdom-config.js"
    ],
    "babel": {
      "testOptions": {
        "babelrc": true
      }
    }
  },
  "dependencies": {
    "classnames": "2.2.5",
    "date-fns": "2.16.1",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "node-sass": "4.11.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "4.4.8",
    "react-router": "3.2.1",
    "redux": "3.6.0",
    "redux-actions": "2.2.1",
    "redux-promise": "0.5.0",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "ava": "1.3.1",
    "@babel/core": "7.4.3",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.0",
    "cross-env": "5.1.4",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.12.1",
    "husky": "1.3.1",
    "redux-test-utils": "0.3.0",
    "parcel-bundler": "1.12.3",
    "rimraf": "2.3.4",
    "sass": "1.18.0",
    "jsdom": "11.3.0"
  }
}
